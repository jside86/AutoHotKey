;------------------------------------------------------------------------------
; GLOBAL TOOLS (GENERAL PRODUCTIVITY)
;------------------------------------------------------------------------------

; Seach google the selected text - Ctrl+Shift+C 
^+c::
{
    A_Clipboard := ""             ; Clear clipboard
    Send("^c")                    ; Copy selected text
    if ClipWait(2)                ; Wait up to 2s for text to arrive
        Run("https://www.google.com/search?q=" . A_Clipboard)
}

; Opens your Documents folder - Ctrl+Alt+D 
^!d::Run(EnvGet("USERPROFILE") . "\Downloads")  

; Copy selected text, open and query Gemini - Alt + G
#Requires AutoHotkey v2.0

!g:: ; Alt + G
{
    ; 1. Copy the highlighted text
    A_Clipboard := "" 
    Send("^c")
    if !ClipWait(2)
        return

    ; 2. Open Gemini
    Run("https://gemini.google.com/app")

    ; 3. Wait for the browser window to appear (Max 5 seconds)
    if WinWaitActive("ahk_exe chrome.exe", , 5) or WinWaitActive("ahk_exe msedge.exe", , 5)
    {
        Sleep(1500) ; Wait 1.5s for the page elements to actually load
        
        ; 4. Paste the text
        Send("^v")
        
        Sleep(300) ; Tiny pause to ensure paste is finished
        
        ; 5. Press Enter to send the query
        Send("{Enter}")
    }
}

;  Make selected text UPPERCASE - Alt + Shift + U
+!u::
{
    A_Clipboard := ""               ; Clear clipboard
    Send("^c")                      ; Copy selection
    if ClipWait(2)                  ; Wait up to 2s for text
    {
        A_Clipboard := StrUpper(A_Clipboard)
        Send("^v")                  ; Paste uppercase text
    }
}

; Make selected text lowercase - Alt + Shift + L
+!l::
{
    A_Clipboard := ""
    Send("^c")
    if ClipWait(2)
    {
        A_Clipboard := StrLower(A_Clipboard)
        Send("^v")
    }
}

; Change the transparency of current windows to 128 and back - Alt+T
!t::  
{
    currentTrans := WinGetTransparent("A")
    if (currentTrans = 128)
        WinSetTransparent(255, "A")
    else
        WinSetTransparent(128, "A")
}

;------------------------------------------------------------------------------
; MEDIA & HARDWARE CONTROL
;------------------------------------------------------------------------------

; Play/Pause Media - Ctrl+Alt+Space
^!Space::Send("{Media_Play_Pause}")

; Previous Media - Ctrl+Alt+Left
^!Left::Send("{Media_Prev}")

; Next Media- Ctrl+Alt+Right
^!Right::Send("{Media_Next}")

; Up and down volume - Alt + Scroll Wheel
!WheelUp::Send("{Volume_Up}")
!WheelDown::Send("{Volume_Down}")

; Stacked Hotkeys: Both trigger the code below
^!m::             ; Ctrl + Alt + M
!#1::       ; Alt + Mouse Side Button (Back)
{
    SoundSetMute(-1) ; Toggle Mute
    
    ; Bigger notification as requested
    isMuted := SoundGetMute()
    ToolTip(isMuted ? "`n  üîá SOUND MUTED  `n " : "`n  üîä SOUND ON  `n ")
    SetTimer(() => ToolTip(), -2000)
}

; Toggle the audio source between speaker and headset -  Ctrl + Alt + S
#Requires AutoHotkey v2.0

^!s:: 
{
    static toggle := 0
    
    if (toggle = 0) {
        ; Use the new name for the headset
        Run("nircmd setdefaultsounddevice Headset",, "Hide")
        ToolTip("Audio: Headset (Logitech)")
        toggle := 1
    } else {
        ; Use the new name for the desktop speakers
        Run("nircmd setdefaultsounddevice `"Desktop Speakers`"",, "Hide")
        ToolTip("Audio: Desktop Speakers (Realtek)")
        toggle := 0
    }
    
    SetTimer () => ToolTip(), -1500 
}

;------------------------------------------------------------------------------
; APPLICATION-SPECIFIC LOGIC
;------------------------------------------------------------------------------

; kills the script - Ctrl+Alt+Esc
^!Escape::ExitApp() 

#Requires AutoHotkey v2.0

; While VS Code is active to Save AND Reload the script - Ctrl + S
#HotIf WinActive("ahk_exe Code.exe")
^s::
{
    Send("^s") ; Sends the native Save command to VS Code
    Sleep(300) ; Brief pause to ensure the file is written to the drive
    Reload()   ; Restarts the script with the new changes
}
#HotIf

;------------------------------------------------------------------------------
; Application-Specific Logic
